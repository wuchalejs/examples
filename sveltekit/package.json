{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"svelte": "^5.37.3",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"vite": "^7.0.6"
	},
	"dependencies": {
		"@wuchale/svelte": "^0.13.0",
		"@wuchale/vite-plugin": "^0.13.0",
		"wuchale": "^0.14.0"
	}
}
