{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.36.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.3",
		"svelte": "^5.38.2",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"vite": "^7.1.3"
	},
	"dependencies": {
		"@wuchale/svelte": "^0.13.2",
		"@wuchale/vite-plugin": "^0.13.1",
		"wuchale": "^0.14.5"
	}
}
