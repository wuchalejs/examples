{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"installLatest": "npm i wuchale@latest @wuchale/svelte@latest @wuchale/vite-plugin@latest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"embedded": "^1.1.0",
		"sass": "^1.97.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
		"vite": "^7.3.0"
	},
	"dependencies": {
		"@webcontainer/env": "^1.1.1",
		"@wuchale/svelte": "^0.18.1",
		"@wuchale/vite-plugin": "^0.16.2",
		"sv-lib": "file:sv-lib-0.0.1.tgz",
		"wuchale": "^0.19.2"
	}
}
