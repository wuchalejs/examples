{
	"name": "sveltekit-advanced",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"installLatest": "npm i wuchale@latest @wuchale/svelte@latest @wuchale/vite-plugin@latest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"extract": "wuchale",
		"clean": "wuchale --clean"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/vite": "^4.1.14",
		"svelte": "^5.43.4",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"vite": "^7.1.9"
	},
	"dependencies": {
		"@wuchale/svelte": "^0.17.4",
		"@wuchale/vite-plugin": "^0.15.3",
		"tailwindcss": "^4.1.12",
		"wuchale": "^0.18.3"
	}
}
