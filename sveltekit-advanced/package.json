{
	"name": "sveltekit-advanced",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"installLatest": "npm i wuchale@latest @wuchale/svelte@latest @wuchale/vite-plugin@latest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"extract": "wuchale",
		"clean": "wuchale --clean"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@webcontainer/env": "^1.1.1",
		"@wuchale/svelte": "^0.18.1",
		"@wuchale/vite-plugin": "^0.16.3",
		"tailwindcss": "^4.1.12",
		"wuchale": "^0.19.4"
	}
}
